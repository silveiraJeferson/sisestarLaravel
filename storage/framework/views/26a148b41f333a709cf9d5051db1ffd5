<?php $__env->startSection('content'); ?>
<?php foreach($consulta as $funcionario): ?>
<img id="imgPerfil"class="thumb thumbnail"src="<?php echo e(url('/imagem/arquivo/'.$funcionario->foto)); ?>"/>
